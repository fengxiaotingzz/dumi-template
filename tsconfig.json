{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "xvnext-components": ["src"],
      "xvnext-components/*": ["src/*", "*"],
      "@/utils/*": ["src/utils/*"],
      "@/icon/*": ["src/icon/*"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*"]
}
