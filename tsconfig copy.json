{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "xvnext-components": ["docs/src"],
      "xvnext-components/*": ["docs/src/*", "*"],
      "@/utils/*": ["docs/src/utils/*"],
      "@/icon/*": ["docs/src/icon/*"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "docs/src/**/*"]
}
